{
  "name": "@jamesgopsill/gtr-client",
  "version": "1.0.0",
  "description": "A Typescript Client for the gateway to research (gtr) UKRI API.",
  "keywords": [
    "Gateway to Research",
    "GtR",
    "UKRI",
    "EPSRC",
    "ESRC",
    "InnovateUK"
  ],
  "main": "dist/index.js",
  "author": "James Gopsill",
  "license": "MIT",
  "private": false,
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesgopsill/gtr-client"
  },
  "bugs": {
    "url": "https://github.com/jamesgopsill/gtr-client/issues"
  },
  "homepage": "https://jamesgopsill.github.io/gtr-client",
  "scripts": {
    "test": "pnpm jest",
    "build": "tsc",
    "build:docs": "typedoc",
    "format": "prettier --write src/**/* && prettier --write __tests__/**/* && prettier --write examples/**/*",
    "pre-add": "pnpm format && pnpm build:docs && node utils/clean-dist.mjs && pnpm build && node utils/clean-crlf.mjs && pnpm test",
    "example": "ts-node examples/get-funds.ts"
  },
  "dependencies": {
    "cross-fetch": "^3.1.4"
  },
  "devDependencies": {
    "@jest/types": "^28.1.3",
    "@tsconfig/node16": "^1.0.2",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.25",
    "glob": "^7.2.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^2.3.4",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  }
}
